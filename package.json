{
  "name": "assurantis-app",
  "version": "1.0.0",
  "description": "Application web statique pour courtier en assurances ASSURANTIS",
  "author": "ASSURANTIS Team",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "ng serve --open",
    "build": "ng build",
    "build:prod": "ng build --configuration production --optimization --aot --build-optimizer --output-hashing all",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "format": "prettier --write \"src/**/*.{ts,html,css,scss,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,css,scss,json}\"",
    "analyze": "ng build --configuration production --stats-json && webpack-bundle-analyzer dist/assurantis-app/browser/stats.json",
    "preview": "ng serve --configuration production",
    "deploy:ghpages": "ng build --configuration production --base-href \"/assurantis-app/\" && npx angular-cli-ghpages --dir=dist/assurantis-app/browser",
    "deploy:netlify": "ng build --configuration production && netlify deploy --prod --dir=dist/assurantis-app/browser",
    "deploy:vercel": "ng build --configuration production && vercel --prod",
    "openapi-gen": "curl -o openapi.json http://localhost:8080/v3/api-docs && ng-openapi-gen --config ng-openapi-gen.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "lucide-angular": "^0.548.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.6.0",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.0",
    "@angular/cli": "^18.0.0",
    "@angular/compiler-cli": "^18.0.0",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.21",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-openapi-gen": "^1.0.5",
    "postcss": "^8.5.6",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.4.0"
  },
  "optionalDependencies": {
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "webpack-bundle-analyzer": "^4.10.0"
  }
}
